<header class="page-header">
  <h1>Signup!</h1>
</header>

<div class="jumbotron">
<%= form_for :user, url: '/users' do |f| %>

<div class="field">
   First Name: <br>
   <%= f.text_field :first_name %>
 </div>
 <div class="field">
   Last Name:<br>
   <%= f.text_field :last_name %>
 </div>
 <div class="field">
   Email: <br>
   <%= f.text_field :email %>
 </div>
 <div class="field">
   Password: <br>
   <%= f.password_field :password %>
 </div>
 <div class="field">
   Password Confirmation: <br>
   <%= f.password_field :password_confirmation %>
 </div>
 <br>
   <%= f.submit "Submit" %>

<% end %>
</div>
